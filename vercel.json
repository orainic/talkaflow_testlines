{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/voice-agent.html"
    },
    {
      "source": "/hotel",
      "destination": "/voice-agent.html?agent=hotel"
    },
    {
      "source": "/clinic",
      "destination": "/voice-agent.html?agent=clinic"
    },
    {
      "source": "/aftersales",
      "destination": "/voice-agent.html?agent=aftersales"
    },
    {
      "source": "/restaurant",
      "destination": "/voice-agent.html?agent=restaurant"
    },
    {
      "source": "/hairstudio",
      "destination": "/voice-agent.html?agent=hairstudio"
    },
    {
      "source": "/hepa",
      "destination": "/voice-agent.html?agent=hepa"
    },
    {
      "source": "/api/ultravox",
      "destination": "/api/ultravox.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}